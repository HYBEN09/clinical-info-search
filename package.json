{
  "name": "clinical-information-search-bar",
  "version": "1.0.0",
  "description": "한국임상정보 검색창 구현 + 검색어 추천 기능 구현 + 캐싱 기능 구현",
  "scripts": {
    "start": "concurrently \"npm run dev\" \"npm run serve\"",
    "serve": "json-server --watch db.json --port 4000",
    "dev": "webpack serve --config ./webpack/config.js --env env=dev",
    "build": "webpack build --config ./webpack/config.js --env env=prod",
    "test": "jest",
    "postinstall": "husky install",
    "format": "prettier --cache --write .",
    "lint": "eslint --cache ."
  },
  "license": "ISC",
  "dependencies": {
    "axios": "^1.5.0",
    "json-server": "0.17.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.15.0",
    "styled-components": "6.0.7",
    "styled-reset": "4.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.22.11",
    "@babel/preset-env": "^7.22.10",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.11",
    "@types/lodash": "4.14.198",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "babel-loader": "^9.1.3",
    "concurrently": "8.2.1",
    "copy-webpack-plugin": "11.0.0",
    "css-loader": "^6.8.1",
    "dotenv-webpack": "8.0.1",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "html-webpack-plugin": "^5.5.3",
    "husky": "^8.0.3",
    "prettier": "^3.0.2",
    "react-icons": "4.10.1",
    "style-loader": "^3.3.3",
    "typescript": "^5.2.2",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "5.9.0"
  },
  "keywords": [
    "webpack",
    "react",
    "typescript"
  ]
}
